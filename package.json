{
  "name": "paperprograms",
  "version": "0.0.8",
  "description": "Co-design environment for accessible educational simulations.",
  "author": "PhET Interactive Simulations <phethelp@colorado.edu>, Jan Paul Posma <j@npaulpos.ma>",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "createdb": "psql -c \"SELECT 1\" paper_programs_development 2>/dev/null 1>/dev/null || createdb paper_programs_development",
    "dev": "npm run createdb && npm run migrate && npm start",
    "dev:docker": "npm run migrate && npm start",
    "dropdb": "dropdb --if-exists paper_programs_development",
    "get-programs": "node scripts/db/get-programs-from-database.js",
    "heroku-postbuild": "webpack",
    "build-react": "webpack --config webpack.config.js",
    "build-server": "webpack --config webpack.server.config.js",
    "lint": "eslint . --rulesdir ../chipper/eslint/rules",
    "lint-fix": "eslint . --fix",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "start": "node server/entry-server.js",
    "test": "npm run lint",
    "start-forge": "electron-forge start",
    "package": "electron-forge package",
    "afterpackage": "node scripts/create-env-file.js",
    "build": "npm run build-react && npm run build-server && npm run package && npm run afterpackage",
    "make": "echo '\"electron-forge make\" is not supported at this time. See https://github.com/phetsims/paper-land/issues/259'"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "extraResource": [
          "server-dist",
          "www"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "engines": {},
  "cacheDirectories": [
    ".babel-cache"
  ],
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-react": "^7.18.6",
    "acorn": "^8.10.0",
    "acorn-walk": "^8.3.2",
    "babel-loader": "^9.1.2",
    "blob-stream": "^0.1.3",
    "bootstrap": "^5.2.3",
    "bootstrap-switch-button-react": "^1.2.0",
    "brfs": "^2.0.2",
    "color-diff": "~1.1.0",
    "css-loader": "^0.28.10",
    "d3": "^5.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "heroku-ssl-redirect": "0.0.4",
    "json-loader": "^0.5.7",
    "knex": "^2.4.0",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "multer": "^1.4.5-lts.1",
    "nocache": "^2.0.0",
    "node-matrices": "^1.0.0",
    "openai": "^4.3.1",
    "pdfkit": "^0.10.0",
    "pg": "^8.0.3",
    "randomcolor": "^0.5.3",
    "react": "^16.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^16.2.0",
    "react-dropzone": "^14.2.3",
    "react-monaco-editor": "^0.13.0",
    "react-syntax-highlighter": "^15.5.0",
    "shuffle-seed": "^1.1.6",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^0.19.1",
    "transform-loader": "^0.2.4",
    "uuid": "^3.2.1",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0",
    "xhr": "^2.4.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "~7.19.1",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "copy-webpack-plugin": "^4.6.0",
    "electron": "^31.0.2",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "~8.28.0",
    "eslint-plugin-react": "^7.31.11",
    "nodemon": "^1.17.1",
    "qunit": "~2.19.4",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
